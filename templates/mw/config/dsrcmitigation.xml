<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<service start="false" instance="0">DsrcMitigation</service>
	<config>
		<!-- VPD cache ID -->
		<cache VDPCacheId="0"/>
		
		<!-- Distance at which the mitigation will be active or not in meters-->
		<distance value="100"/>
		
		<!-- Application ID and CAM ID for Choir Protocol -->
		<pubsub incam="2" applicationId="96" messageId="6"/>
		
		<!-- Indicates the numbe of station defined -->
		<index value="1"/>
		<!-- Defines the position of a toll station -->
		<station0 lat="48.11520" lon="-1.40137"/>
	</config>
</configuration>
