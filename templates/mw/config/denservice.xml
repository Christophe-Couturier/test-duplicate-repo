<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<service start="true" instance="0">DenService</service>
	<config>
		<!-- Application Id for choir protocol -->
		<pubsub senderid="91" outdenm="5" triggerdenm="25" handlerid="93"
		        receiverid="92" indenm="15" injsondenm="35" outjsondenm="45" answerdenm="55"/>

		<!-- Enable the DENM forwarding service -->
		<forwarding value="$forwarding"/>

		<!-- Enable ActionID forcing -->
		<forceactionid enabled="$forceactionid"/>

		<!-- Enable autoUpdate of DENM -->
		<autoupdate enabled="$autoupdate"/>

		<!-- Cache for DENM -->
		<cache originating="2" receiving="21" forward="22"/>
	</config>
</configuration>
