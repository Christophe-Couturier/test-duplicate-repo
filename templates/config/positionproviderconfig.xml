<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<service start="true" instance="0">PositionProvider</service>
	<config>
		<!-- Message Id for choir protocol -->
		<pubsub positionmessageId="200"  />
		<!-- position provider static/gpsd  time provider system/gpd-->
		<provider position="static" time="system"/>
		<!-- specific configuration for position provider -->
		<staticposition latitude="48.837586" longitude="2.101369" refreshintervalms="1000" />
		<gpsd address="localhost" port="2947" />
	</config>
</configuration>
